{
  "datepickerConfigurationOptions": [
    {
      "property": "selectedDate",
      "type": "Date",
      "default": "null",
      "description": "Selected date value"
    },
    {
      "property": "minDate",
      "type": "Date",
      "default": "null",
      "description": "Minimum selectable date"
    },
    {
      "property": "maxDate",
      "type": "Date",
      "default": "null",
      "description": "Maximum selectable date"
    },
    {
      "property": "myFilter",
      "type": "Function",
      "default": "Disables weekends",
      "description": "Custom date filter logic"
    },
    {
      "property": "enableFilter",
      "type": "boolean",
      "default": "true",
      "description": "Enable or disable date filter"
    }
  ],
  "datepickerOutputEvents": [
    {
      "event": "selectedChange",
      "type": "Date",
      "description": "Emits when a single date is selected"
    },
    {
      "event": "rangeChange",
      "type": "{start: Date, end: Date}",
      "description": "Emits when a date range is selected"
    },
    {
      "event": "monthSelected",
      "type": "Date",
      "description": "Emits when a month is selected"
    },
    {
      "event": "yearSelected",
      "type": "Date",
      "description": "Emits when a year is selected"
    }
  ]
}
